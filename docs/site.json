{
  "baseUrl": "/tp",
  "titlePrefix": "TAfinder",
  "titleSuffix": "",
  "faviconPath": "images/TAfinderLogo.png",
  "style": {
    "codeTheme": "light"
  },
  "ignore": [
    "_markbind/layouts/*",
    "_markbind/logs/*",
    "_site/*",
    "site.json",
    "*.md",
    "*.njk",
    ".git/*",
    "node_modules/*"
  ],
  "pagesExclude": [
    "node_modules/*"
  ],
  "pages": [
    {
      "glob": [
        "**/default.md",
        "**/*.md"
      ]
    }
  ],
  "deploy": {
    "message": "Site Update."
  },
  "timeZone": "Asia/Singapore"
}
